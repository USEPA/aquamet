citHeader("To cite the aquamet package in publications use:")

citEntry(
   entry="Manual",
   title="aquamet: Aquatic Metrics Calculation",
   author="Curt W. Seeliger, Karen A. Blocksom, and Thomas M. Kincaid",
   year=2021,
   note="R package version 2.5.4",
   url="http://www.epa.gov/nheerl/arm/",
   textVersion=paste("Seeliger, C. W., Blocksom, K. A., and Kincaid, T. M. (2021).",
                      "aquamet: Aquatic Metrics Calculation.",
                      "R package version 2.4.",
                      "URL: http://www.epa.gov/nheerl/arm/.")
)
